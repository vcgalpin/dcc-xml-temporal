<dcc:digitalCalibrationCertificate xmlns:dcc="https://ptb.de/dcc" xmlns:si="https://ptb.de/si" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" schemaVersion="3.1.2" xsi:schemaLocation="https://ptb.de/dcc dcc.xsd">
    <dcc:administrativeData>
        <dcc:dccSoftware>
            <dcc:software>
                <dcc:name>
                    <dcc:content>GenerateDCC_v1.py</dcc:content>
                </dcc:name>
                <dcc:release>v1.0</dcc:release>
                <dcc:description>
                    <dcc:content>Python software to generate DCC</dcc:content>
                </dcc:description>
            </dcc:software>
            <dcc:software>
                <dcc:name>
                    <dcc:content>InformationAndResults_v1.xlsx</dcc:content>
                </dcc:name>
                <dcc:release>v1.0</dcc:release>
                <dcc:description>
                    <dcc:content>Microsoft Excel workbook containing administrative information and measurement results</dcc:content>
                </dcc:description>
            </dcc:software>
        </dcc:dccSoftware>
        <dcc:coreData>
            <dcc:countryCodeISO3166_1>GB</dcc:countryCodeISO3166_1>
            <dcc:usedLangCodeISO639_1>en</dcc:usedLangCodeISO639_1>
            <dcc:mandatoryLangCodeISO639_1>en</dcc:mandatoryLangCodeISO639_1>
            <dcc:uniqueIdentifier>xxxxxx</dcc:uniqueIdentifier>
            <dcc:beginPerformanceDate>2022-08-15</dcc:beginPerformanceDate>
            <dcc:endPerformanceDate>2022-08-15</dcc:endPerformanceDate>
            <dcc:performanceLocation>laboratory</dcc:performanceLocation>
        </dcc:coreData>
        <dcc:items>
            <dcc:item>
                <dcc:name>
                    <dcc:content>Digital thermometer</dcc:content>
                </dcc:name>
                <dcc:manufacturer>
                    <dcc:name>
                        <dcc:content>Manufacturer</dcc:content>
                    </dcc:name>
                </dcc:manufacturer>
                <dcc:model>ABC 1234</dcc:model>
                <dcc:identifications>
                    <dcc:identification>
                        <dcc:issuer>customer</dcc:issuer>
                        <dcc:value>000000</dcc:value>
                        <dcc:name>
                            <dcc:content>Customer asset ID number</dcc:content>
                        </dcc:name>
                    </dcc:identification>
                </dcc:identifications>
            </dcc:item>
        </dcc:items>
        <dcc:calibrationLaboratory>
            <dcc:contact>
                <dcc:name>
                    <dcc:content>The Metrology Lab</dcc:content>
                </dcc:name>
                <dcc:eMail>metlab@example.org</dcc:eMail>
                <dcc:location>
                    <dcc:street>First Road</dcc:street>
                    <dcc:city>Bigtown</dcc:city>
                    <dcc:state>Smallcounty</dcc:state>
                    <dcc:postCode>BG5 6XZ</dcc:postCode>
                    <dcc:countryCode>GB</dcc:countryCode>
                </dcc:location>
            </dcc:contact>
        </dcc:calibrationLaboratory>
        <dcc:respPersons>
            <dcc:respPerson>
                <dcc:person>
                    <dcc:name>
                        <dcc:content>A. Person</dcc:content>
                    </dcc:name>
                </dcc:person>
                <dcc:role>Authorisation</dcc:role>
                <dcc:mainSigner>true</dcc:mainSigner>
            </dcc:respPerson>
            <dcc:respPerson>
                <dcc:person>
                    <dcc:name>
                        <dcc:content>A.N. Employee</dcc:content>
                    </dcc:name>
                </dcc:person>
                <dcc:role>Measurement</dcc:role>
            </dcc:respPerson>
        </dcc:respPersons>
        <dcc:customer>
            <dcc:name>
                <dcc:content>AB Production</dcc:content>
            </dcc:name>
            <dcc:eMail>ab_prod@example.com</dcc:eMail>
            <dcc:location>
                <dcc:street>Hardtofind Terrace</dcc:street>
                <dcc:city>NiceTown</dcc:city>
                <dcc:postCode>NT7 8DP</dcc:postCode>
                <dcc:countryCode>GB</dcc:countryCode>
                <dcc:street>Second Avenue</dcc:street>
            </dcc:location>
        </dcc:customer>
        <dcc:statements>
            <dcc:statement>
                <dcc:declaration>
                    <dcc:content>Date on or before which the item is to be recalibrated.</dcc:content>
                </dcc:declaration>
                <dcc:date>2023-07-01</dcc:date>
            </dcc:statement>
            <dcc:statement>
                <dcc:declaration>
                    <dcc:content>Measurement resolution.</dcc:content>
                </dcc:declaration>
                <dcc:data>
                    <dcc:quantity>
                        <si:real>
                            <si:value>0.1</si:value>
                            <si:unit>\degreeCelsius</si:unit>
                        </si:real>
                    </dcc:quantity>
                </dcc:data>
            </dcc:statement>
            <dcc:statement>
                <dcc:declaration>
                    <dcc:content>The digital thermometer and associated probe were submitted for calibration over the specified temperature range in terms of the International Temperature Scale of 1990.</dcc:content>
                </dcc:declaration>
                <dcc:data>
                    <dcc:quantity>
                        <si:real>
                            <si:label>Lower limit</si:label>
                            <si:value>0.0</si:value>
                            <si:unit>\degreeCelsius</si:unit>
                        </si:real>
                    </dcc:quantity>
                    <dcc:quantity>
                        <si:real>
                            <si:label>Upper limit</si:label>
                            <si:value>150.0</si:value>
                            <si:unit>\degreeCelsius</si:unit>
                        </si:real>
                    </dcc:quantity>
                </dcc:data>
            </dcc:statement>
        </dcc:statements>
    </dcc:administrativeData>
    <dcc:measurementResults>
        <dcc:measurementResult>
            <dcc:name>
                <dcc:content>Measurement results</dcc:content>
            </dcc:name>
            <dcc:influenceConditions>
                <dcc:influenceCondition refType="gp_immersionDepth">
                    <dcc:name>
                        <dcc:content>Immersion depth in dry block calibrator</dcc:content>
                    </dcc:name>
                    <dcc:data>
                        <dcc:quantity>
                            <dcc:name>
                                <dcc:content>Immersion depth</dcc:content>
                            </dcc:name>
                            <si:real>
                                <si:value>0.1</si:value>
                                <si:unit>\metre</si:unit>
                            </si:real>
                        </dcc:quantity>
                    </dcc:data>
                </dcc:influenceCondition>
                <dcc:influenceCondition refType="basic_temperature">
                    <dcc:name>
                        <dcc:content>Ambient temperature</dcc:content>
                    </dcc:name>
                    <dcc:data>
                        <dcc:quantity refType="basic_temperatureMin">
                            <dcc:name>
                                <dcc:content>Lower limit</dcc:content>
                            </dcc:name>
                            <si:real>
                                <si:value>20.0</si:value>
                                <si:unit>\degreeCelsius</si:unit>
                            </si:real>
                        </dcc:quantity>
                        <dcc:quantity refType="basic_temperatureMax">
                            <dcc:name>
                                <dcc:content>Upper limit</dcc:content>
                            </dcc:name>
                            <si:real>
                                <si:value>24.0</si:value>
                                <si:unit>\degreeCelsius</si:unit>
                            </si:real>
                        </dcc:quantity>
                    </dcc:data>
                </dcc:influenceCondition>
            </dcc:influenceConditions>
            <dcc:results>
                <dcc:result>
                    <dcc:name>
                        <dcc:content>Results</dcc:content>
                    </dcc:name>
                    <dcc:data>
                        <dcc:list refType="Results table">
                            <dcc:quantity refType="basic_referenceValue">
                                <dcc:name>
                                    <dcc:content>ITS-90 temperature</dcc:content>
                                </dcc:name>
                                <si:realListXMLList>
                                    <si:valueXMLList>0.00 49.99 100.01 149.99 0.0</si:valueXMLList>
                                    <si:unitXMLList>\degreeCelsius</si:unitXMLList>
                                </si:realListXMLList>
                            </dcc:quantity>
                            <dcc:quantity refType="basic_measuredValue">
                                <dcc:name>
                                    <dcc:content>Measured temperature</dcc:content>
                                </dcc:name>
                                <si:realListXMLList>
                                    <si:valueXMLList>0.0 49.9 100.0 149.9 0.0</si:valueXMLList>
                                    <si:unitXMLList>\degreeCelsius</si:unitXMLList>
                                    <si:expandedUncXMLList>                                        <si:uncertaintyXMLList>0.2</si:uncertaintyXMLList>                                        <si:coverageFactorXMLList>2.0</si:coverageFactorXMLList>                                        <si:coverageProbabilityXMLList>0.95</si:coverageProbabilityXMLList>                                    </si:expandedUncXMLList>                                </si:realListXMLList>
                            </dcc:quantity>
                        </dcc:list>
                    </dcc:data>
                </dcc:result>
            </dcc:results>
        </dcc:measurementResult>
    </dcc:measurementResults>
</dcc:digitalCalibrationCertificate>